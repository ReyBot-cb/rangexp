#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/rangexp/node_modules/.pnpm/expo@52.0.48_@babel+core@7.28.6_@babel+preset-env@7.28.6_@babel+core@7.28.6__@expo+metro-runt_ao4sr7fte7sk5ttqvzsvyjbelq/node_modules/expo/bin/node_modules:/home/ubuntu/rangexp/node_modules/.pnpm/expo@52.0.48_@babel+core@7.28.6_@babel+preset-env@7.28.6_@babel+core@7.28.6__@expo+metro-runt_ao4sr7fte7sk5ttqvzsvyjbelq/node_modules/expo/node_modules:/home/ubuntu/rangexp/node_modules/.pnpm/expo@52.0.48_@babel+core@7.28.6_@babel+preset-env@7.28.6_@babel+core@7.28.6__@expo+metro-runt_ao4sr7fte7sk5ttqvzsvyjbelq/node_modules:/home/ubuntu/rangexp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/rangexp/node_modules/.pnpm/expo@52.0.48_@babel+core@7.28.6_@babel+preset-env@7.28.6_@babel+core@7.28.6__@expo+metro-runt_ao4sr7fte7sk5ttqvzsvyjbelq/node_modules/expo/bin/node_modules:/home/ubuntu/rangexp/node_modules/.pnpm/expo@52.0.48_@babel+core@7.28.6_@babel+preset-env@7.28.6_@babel+core@7.28.6__@expo+metro-runt_ao4sr7fte7sk5ttqvzsvyjbelq/node_modules/expo/node_modules:/home/ubuntu/rangexp/node_modules/.pnpm/expo@52.0.48_@babel+core@7.28.6_@babel+preset-env@7.28.6_@babel+core@7.28.6__@expo+metro-runt_ao4sr7fte7sk5ttqvzsvyjbelq/node_modules:/home/ubuntu/rangexp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../expo/bin/autolinking" "$@"
else
  exec node  "$basedir/../expo/bin/autolinking" "$@"
fi
